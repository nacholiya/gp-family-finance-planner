{
  "name": "beanies-family",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run security:audit && vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:report": "playwright show-report",
    "translate": "node scripts/updateTranslations.mjs",
    "translate:zh": "node scripts/updateTranslations.mjs zh",
    "lint": "npm run lint:eslint && npm run lint:style",
    "lint:eslint": "eslint . --ext .vue,.js,.ts,.tsx",
    "lint:style": "stylelint \"src/**/*.{vue,css}\"",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:style -- --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "vue-tsc --noEmit",
    "security:check": "npm run security:audit && npm run security:lint",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:lint": "eslint . --ext .vue,.js,.ts,.tsx --config eslint.security.config.js",
    "security:full": "npm run security:audit && npm run security:lint && npm run type-check",
    "prepare": "husky"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.16",
    "chart.js": "^4.5.1",
    "idb": "^8.0.3",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@heroicons/vue": "^2.2.0",
    "@microsoft/eslint-plugin-sdl": "^1.1.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-secrets": "^2.2.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^9.33.0",
    "fake-indexeddb": "^6.2.5",
    "happy-dom": "^20.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^3.1.4"
  }
}
